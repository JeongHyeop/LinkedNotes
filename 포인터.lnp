\par
//\'c6\'f7\'c0\'ce\'c5\'cd\'c7\'fc \'ba\'af\'bc\'f6\'b4\'c2 \'b4\'d9\'b8\'a5 \'ba\'af\'bc\'f6\'c0\'c7 \'c1\'d6\'bc\'d2\'b0\'aa\'b8\'b8 \'b9\'de\'c0\'bb \'bc\'f6\'c0\'d6\'b4\'d9.\par
\f1 ex)*a = &amp;value;\par
\par
